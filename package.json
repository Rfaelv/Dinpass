  "license": "ISC",
  "devDependencies": {
    "electron": "10.1.5",
    "electron-packager": "^15.1.0",
    "electron-wix-msi": "^3.0.4"
  },
  "dependencies": {
    "child-process": "^1.0.2",
    "fs": "0.0.1-security",
    "materialize-css": "^1.0.0-rc.2",
    "path": "^0.12.7"
  }
